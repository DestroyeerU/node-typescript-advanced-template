{
  "name": "node-typescript-advanced-template",
  "version": "1.0.0",
  "main": "src/server.ts",
  "repository": "https://github.com/DestroyeerU/node-typescript-advanced-template.git",
  "author": "DestroyeerU <destroyeeru18@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register src/server.ts",
    "dev:debug": "nodemon --inspect src/server.js",

    "migrate": "dotenv -e .env.test -- npx prisma migrate save -c --experimental;dotenv -e .env.test -- npx prisma migrate up --experimental;npx prisma generate",
    "migrate:save": "dotenv -e .env.test -- prisma migrate save -c --experimental",
    "migrate:up": "dotenv -e .env.test -- prisma migrate up --experimental",
    "migrate:reset": "dotenv -e .env.test -- prisma migrate down `20200610103043-create-users` --experimental",
    "generate": "dotenv -e .env.test -- prisma generate --experimental",

    "pretest": "dotenv -e .env.test -- prisma migrate up -c --experimental;dotenv -e .env.test -- prisma generate",
    "test": "NODE_ENV=test dotenv -e .env.test -- jest",
    "posttest": "dotenv -e .env.test -- prisma migrate down `20200610103043-create-users` --experimental"
  },
  "dependencies": {
    "@prisma/client": "^2.0.0-beta.7",
    "@sucrase/jest-plugin": "^2.0.0",
    "@types/multer": "^1.4.3",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.15",
    "multer": "^1.4.2",
    "yup": "^0.29.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@prisma/cli": "^2.0.0-beta.7",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.6",
    "@types/faker": "^4.1.12",
    "@types/jest": "^25.2.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.20",
    "@types/supertest": "^2.0.9",
    "@types/yup": "^0.29.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "babel-jest": "^25.5.0",
    "eslint": "^6.8.0",
    "eslint-config": "^0.3.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "faker": "^4.1.0",
    "jest": "^25.5.0",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  }
}
